views:
  - type: table
    name: Table
    filters:
      and:
        - file.tags.containsAll("DSA", "Leetcode")
    order:
      - file.name
      - topics
      - file.tags
      - file.ctime
    sort:
      - property: file.ctime
        direction: DESC
      - property: file.tags
        direction: DESC
    columnSize:
      file.name: 265
      note.topics: 151
      file.tags: 388
